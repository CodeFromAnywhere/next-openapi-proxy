    "x-apisguru-categories": [
      "analytics"
    ],
    "x-logo": {
      "url": "https://static.redeal.se/APIDefinitions/re-deal-logo-cmyk_1.svg"
    },
    "x-origin": [
      {
        "format": "openapi",
        "url": "https://static.redeal.se/APIDefinitions/analytics.redeal.io-1.0.0-swagger.yaml",
        "version": "3.0"
      }
    ],
    "x-providerName": "redeal.io",
    "x-serviceName": "analytics"
  },
  "tags": [
    {
      "name": "developers",
      "description": "Operations available to regular developers"
    }
  ],
  "paths": {
    "/events": {
      "get": {
        "tags": [
          "developers"
        ],
        "summary": "get events for analytics",
        "operationId": "getEvents",
        "description": "By passing in the company, site or deal Id a set of user interaction event records is returned. For pagination of a large result set use queryexecutionid and nexttoken instead.\n",
        "parameters": [
          {
            "in": "query",
            "name": "company",
            "description": "pass an optional company Id",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "site",
            "description": "pass an optional site Id",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "deal",
            "description": "pass an optional deal Id",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "type",
            "description": "type of records to return",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "all",
                "clicks",
                "contacts"
              ]
            }
          },
          {
            "in": "query",
            "name": "nexttoken",
            "description": "next token to start returning records from",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "queryexecutionid",
            "description": "id of execution to get more records based on next token",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "description": "from date range",
            "in": "query",
            "name": "from",
            "required": false,
            "example": "2021-01-01",
            "format": "date",
            "pattern": "YYYY-MM-DD",
            "minLength": 4,
            "maxLength": 10,
            "schema": {
              "type": "string"
            }
          },
          {
            "description": "to date range",
            "in": "query",
            "name": "to",
            "required": false,
            "example": "2021-01-01",
            "format": "date",
            "pattern": "YYYY-MM-DD",
            "minLength": 4,
            "maxLength": 10,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "analytics results matching criteria",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/EventRecord"
                  }
                }
              }
            }
          },
          "400": {
            "description": "bad input parameter"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "EventRecord": {
