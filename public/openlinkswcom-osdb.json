        "tags": [
          "OSDB"
        ],
        "responses": {
          "200": {
            "description": "unloadService response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "default": "success",
                      "enum": [
                        "success"
                      ]
                    },
                    "method": {
                      "type": "string",
                      "description": "The name of the REST API method"
                    },
                    "api": {
                      "type": "string",
                      "description": "The path of the REST API method"
                    },
                    "response": {
                      "type": "string",
                      "description": "Confirmation message"
                    }
                  },
                  "required": [
                    "status",
                    "method",
                    "api",
                    "response"
                  ]
                },
                "example": {
                  "status": "success",
                  "method": "unload_service",
